{
    "name": "mrgoodbytes8667/discord-client-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that adds some routes and mechanisms for Discord OAuth",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/annotations": "^1.12",
        "doctrine/collections": "^1.6",
        "mrgoodbytes8667/command-bundle": "^1.3",
        "mrgoodbytes8667/discord-response-bundle": "^0.15 | ^0.16",
        "mrgoodbytes8667/response-bundle": "5.1.x-dev as 5.1.0",
        "symfony/http-client": "^6.2",
        "symfony/security-bundle": "^6.2"
    },
    "require-dev": {
        "doctrine/common": "^3.0",
        "fakerphp/faker": "^1.13",
        "mrgoodbytes8667/test-common": ">=0.2",
        "mrgoodbytes8667/test-common-faker": ">=0.4.2",
        "mrgoodbytes8667/test-discord-faker": ">=0.1 | 0.1.0-BETA2",
        "phpunit/phpunit": "<10",
        "rector/rector": "^0.15",
        "symfony/test-pack": "^1.0.8",
        "symfony/var-dumper": "^6.2"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0",
        "mrgoodbytes8667/enum-serializer-bundle": "<3.2.1",
        "mrgoodbytes8667/response-bundle": "<5.1.0",
        "phpunit/phpunit": "<9.5 || 9.5.8",
        "symfony/console": "<6.2",
        "symfony/framework-bundle": "<6.2",
        "symfony/serializer": "<6.2"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\DiscordClientBundle\\": "src/",
            "Bytes\\DiscordClientBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
