{
    "name": "mrgoodbytes8667/discord-client-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that adds some routes and mechanisms for Discord OAuth",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "doctrine/annotations": "^1.12",
        "doctrine/collections": "^1.6",
        "mrgoodbytes8667/command-bundle": "^1.3",
        "mrgoodbytes8667/discord-response-bundle": "^0.12",
        "symfony/http-client": "^5.3 | ^6.0",
        "symfony/security-core": "^5.3 | ^6.0"
    },
    "require-dev": {
        "doctrine/common": "^3.0",
        "fakerphp/faker": "^1.13",
        "mrgoodbytes8667/test-common": ">=0.0.19",
        "mrgoodbytes8667/test-common-faker": "^0.3.5",
        "mrgoodbytes8667/test-discord-faker": ">=0.0.9",
        "symfony/security-bundle": "^5.3 | ^6.0",
        "symfony/test-pack": "^1.0.8",
        "symfony/var-dumper": "^5.3 | ^6.0"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "suggest": {
        "symfony/security-bundle": ""
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0",
        "mrgoodbytes8667/response-bundle": "<2.1.0",
        "phpunit/phpunit": "<9.5 || 9.5.8",
        "symfony/framework-bundle": "<5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\DiscordClientBundle\\": "src/",
            "Bytes\\DiscordClientBundle\\Tests\\": "Tests/"
        }
    }
}
